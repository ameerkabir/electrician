    "core-js": "^1.2.6",
    "lodash": "^4.7.0"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "pre-commit": "^1.1.2"
  }
}
